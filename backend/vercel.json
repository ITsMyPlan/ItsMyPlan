{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ],
  "env": {
    "DATABASE_URL": "@database_url"
  },
  "env": {
    "MY_SECRET": "my_secret_value"
  },
  "env": {
    "ANOTHER_SECRET": "another_secret_value"
  }
}
