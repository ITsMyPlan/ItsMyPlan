{
	"version": 2,
	"builds": [
		{
			"src": "api/index.js",
			"use": "@vercel/node"
		},
		{
			"src": "my-react-app/package.json",
			"use": "@vercel/static-build",
			"config": {
				"distDir": "build"
			}
		}
	],
	"routes": [
		{
			"src": "/api/(.*)",
			"dest": "/api/index.js"
		},
		{
			"src": "/(.*)",
			"dest": "/my-react-app/build/$1"
		}
	]
}
